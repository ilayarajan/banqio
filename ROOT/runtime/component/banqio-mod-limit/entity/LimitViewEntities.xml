<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-2.1.xsd">

    <view-entity entity-name="FundsTransferLimitDetail" package="banqio.limit">
        <member-entity entity-alias="lmt" entity-name="banqio.limit.FundsTransferLimit"/>
        <member-entity entity-alias="app" entity-name="commons.server.App" join-from-alias="lmt">
            <key-map field-name="appId"/>
        </member-entity>
        <member-entity entity-alias="limitType" entity-name="moqui.basic.Enumeration" join-from-alias="lmt">
            <key-map field-name="limitTypeEnumId"/>
        </member-entity>
        <member-entity entity-alias="limitFrequency" entity-name="moqui.basic.Uom" join-from-alias="lmt">
            <key-map field-name="frequencyUomId"/>
        </member-entity>
        <member-entity entity-alias="transferType" entity-name="banqio.ft.FundsTransferType" join-from-alias="lmt">
            <key-map field-name="transferTypeId"/>
        </member-entity>
        <member-entity entity-alias="currencyType" entity-name="moqui.basic.Enumeration" join-from-alias="lmt">
            <key-map field-name="currencyTypeEnumId"/>
        </member-entity>
        <member-entity entity-alias="transferCurrency" entity-name="moqui.basic.Uom" join-from-alias="lmt" join-optional="true">
            <key-map field-name="transferCurrencyId"/>
        </member-entity>
        <member-entity entity-alias="debitCurrency" entity-name="moqui.basic.Uom" join-from-alias="lmt" join-optional="true">
            <key-map field-name="debitCurrencyId"/>
        </member-entity>
        <member-entity entity-alias="accountLimit" entity-name="banqio.limit.AccountFundsTransferLimit" join-from-alias="lmt" join-optional="true">
            <key-map field-name="limitId"/>
        </member-entity>
        <member-entity entity-alias="beneficiaryLimit" entity-name="banqio.limit.BeneficiaryFundsTransferLimit" join-from-alias="lmt" join-optional="true">
            <key-map field-name="limitId"/>
        </member-entity>
        <member-entity entity-alias="userLimit" entity-name="banqio.limit.UserFundsTransferLimit" join-from-alias="lmt" join-optional="true">
            <key-map field-name="limitId"/>
        </member-entity>
        <member-entity entity-alias="userAccount" entity-name="moqui.security.UserAccount" join-from-alias="userLimit" join-optional="true">
            <key-map field-name="userId"/>
        </member-entity>
        <member-entity entity-alias="userGroupLimit" entity-name="banqio.limit.UserGroupFundsTransferLimit" join-from-alias="lmt" join-optional="true">
            <key-map field-name="limitId"/>
        </member-entity>
        <member-entity entity-alias="userGroup" entity-name="moqui.security.UserGroup" join-from-alias="userGroupLimit" join-optional="true">
            <key-map field-name="userGroupId"/>
        </member-entity>
        <member-entity entity-alias="organizationLimit" entity-name="banqio.limit.OrganizationFundsTransferLimit" join-from-alias="lmt" join-optional="true">
            <key-map field-name="limitId"/>
        </member-entity>
        <member-entity entity-alias="organizationParty" entity-name="mantle.party.Party" join-from-alias="organizationLimit" join-optional="true">
            <key-map field-name="partyId"/>
        </member-entity>
        <member-entity entity-alias="organization" entity-name="mantle.party.Organization" join-from-alias="organizationParty" join-optional="true">
            <key-map field-name="partyId"/>
        </member-entity>

        <alias-all entity-alias="lmt"/>
        <alias name="appName" entity-alias="app" field="appName"/>
        <alias name="accountId" entity-alias="accountLimit" field="accountId"/>
        <alias name="beneficiaryId" entity-alias="beneficiaryLimit" field="beneficiaryId"/>
        <alias name="limitTypeDescription" entity-alias="limitType" field="description"/>
        <alias name="limitTypeSequenceNum" entity-alias="limitType" field="sequenceNum"/>
        <alias name="transferTypeName" entity-alias="transferType" field="typeName"/>
        <alias name="limitFrequencyDescription" entity-alias="limitFrequency" field="description"/>
        <alias name="currencyTypeDescription" entity-alias="currencyType" field="description"/>
        <alias name="transferCurrencyDescription" entity-alias="transferCurrency" field="description"/>
        <alias name="transferCurrencyAbbreviation" entity-alias="transferCurrency" field="abbreviation"/>
        <alias name="transferCurrencyNumericCode" entity-alias="transferCurrency" field="numericCode"/>
        <alias name="debitCurrencyDescription" entity-alias="debitCurrency" field="description"/>
        <alias name="debitCurrencyAbbreviation" entity-alias="debitCurrency" field="abbreviation"/>
        <alias name="debitCurrencyNumericCode" entity-alias="debitCurrency" field="numericCode"/>
        <alias name="userId" entity-alias="userAccount" field="userId"/>
        <alias name="username" entity-alias="userAccount" field="username"/>
        <alias name="userFullName" entity-alias="userAccount" field="userFullName"/>
        <alias name="userGroupId" entity-alias="userGroup" field="userGroupId"/>
        <alias name="userGroupDescription" entity-alias="userGroup" field="description"/>
        <alias name="partyId" entity-alias="organization" field="partyId"/>
        <alias name="externalId" entity-alias="organizationParty" field="externalId"/>
        <alias name="organizationName" entity-alias="organization" field="organizationName"/>
    </view-entity>

</entities>