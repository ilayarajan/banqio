            <parameter name="beneficiaryList" type="List">
                <parameter name="beneficiary" type="Map">
                    <auto-parameters entity-name="banqio.beneficiary.BeneficiaryDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
