<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-2.1.xsd">

    <view-entity entity-name="BeneficiaryTypeDetail" package="banqio.beneficiary">
        <member-entity entity-alias="beneficiaryType" entity-name="banqio.beneficiary.BeneficiaryType"/>
        <member-entity entity-alias="parentType" entity-name="banqio.beneficiary.BeneficiaryType" join-from-alias="beneficiaryType" join-optional="true">
            <key-map field-name="parentTypeId" related="typeId"/>
        </member-entity>
        <member-entity entity-alias="typeBehaviour" entity-name="moqui.basic.Enumeration" join-from-alias="beneficiaryType">
            <key-map field-name="typeBehaviourEnumId"/>
        </member-entity>

        <alias-all entity-alias="beneficiaryType"/>
        <alias name="parentTypeName" entity-alias="parentType" field="typeName"/>
        <alias name="typeBehaviourDescription" entity-alias="typeBehaviour" field="description"/>
    </view-entity>
    <view-entity entity-name="BeneficiaryDetail" package="banqio.beneficiary">
        <member-entity entity-alias="beneficiary" entity-name="banqio.beneficiary.Beneficiary"/>
        <member-entity entity-alias="beneficiaryAddress" entity-name="banqio.beneficiary.BeneficiaryAddress" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="beneficiaryId"/>
        </member-entity>
        <member-entity entity-alias="country" entity-name="moqui.basic.Geo" join-from-alias="beneficiaryAddress" join-optional="true">
            <key-map field-name="countryGeoId"/>
        </member-entity>
        <member-entity entity-alias="beneficiaryType" entity-name="banqio.beneficiary.BeneficiaryType" join-from-alias="beneficiary">
            <key-map field-name="typeId"/>
        </member-entity>
        <member-entity entity-alias="typeBehaviour" entity-name="moqui.basic.Enumeration" join-from-alias="beneficiaryType">
            <key-map field-name="typeBehaviourEnumId"/>
        </member-entity>
        <member-entity entity-alias="beneficiaryStatus" entity-name="moqui.basic.StatusItem" join-from-alias="beneficiary">
            <key-map field-name="statusId"/>
        </member-entity>
        <member-entity entity-alias="preferredCurrency" entity-name="moqui.basic.Uom" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="preferredCurrencyId"/>
        </member-entity>
        <member-entity entity-alias="branch" entity-name="banqio.bank.Branch" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="swiftCode"/>
        </member-entity>
        <member-entity entity-alias="bank" entity-name="banqio.bank.Bank" join-from-alias="branch" join-optional="true">
            <key-map field-name="bankId"/>
        </member-entity>
        <member-entity entity-alias="inputUser" entity-name="moqui.security.UserAccount" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="inputUserId"/>
        </member-entity>
        <member-entity entity-alias="updateUser" entity-name="moqui.security.UserAccount" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="updateUserId"/>
        </member-entity>

        <alias-all entity-alias="beneficiary"/>
        <alias-all entity-alias="beneficiaryAddress">
            <exclude field="beneficiaryId"/>
            <exclude field="creationDate"/>
        </alias-all>
        <alias name="bankId" entity-alias="bank" field="bankId"/>
        <alias name="bankName" entity-alias="bank" field="bankName"/>
        <alias name="countryGeoName" entity-alias="country" field="geoName"/>
        <alias name="countryGeoCodeAlpha2" entity-alias="country" field="geoCodeAlpha2"/>
        <alias name="countryGeoCodeAlpha3" entity-alias="country" field="geoCodeAlpha3"/>
        <alias name="countryGeoCodeNumeric" entity-alias="country" field="geoCodeNumeric"/>
        <alias name="typeName" entity-alias="beneficiaryType" field="typeName"/>
        <alias name="typeBehaviourEnumId" entity-alias="typeBehaviour" field="enumId"/>
        <alias name="typeBehaviourDescription" entity-alias="typeBehaviour" field="description"/>
        <alias name="statusDescription" entity-alias="beneficiaryStatus" field="description"/>
        <alias name="preferredCurrencyDescription" entity-alias="preferredCurrency" field="description"/>
        <alias name="preferredCurrencyAbbreviation" entity-alias="preferredCurrency" field="abbreviation"/>
        <alias name="inputUserFullName" entity-alias="inputUser" field="userFullName"/>
        <alias name="updateUserFullName" entity-alias="updateUser" field="userFullName"/>

        <entity-condition>
            <econditions>
                <econdition field-name="statusId" operator="not-equals" value="BEN_DELETED"/>
                <econdition field-name="disabled" value="N"/>
            </econditions>
        </entity-condition>
    </view-entity>
    <view-entity entity-name="BeneficiaryContactMechDetail" package="banqio.beneficiary">
        <member-entity entity-alias="beneficiaryContactMech" entity-name="banqio.beneficiary.BeneficiaryContactMech"/>
        <member-entity entity-alias="contactMechType" entity-name="moqui.basic.Enumeration" join-from-alias="beneficiaryContactMech">
            <key-map field-name="contactMechTypeEnumId"/>
        </member-entity>
        <member-entity entity-alias="telecomNumber" entity-name="banqio.beneficiary.BeneficiaryTelecomNumber" join-from-alias="beneficiaryContactMech" join-optional="true">
            <key-map field-name="contactMechId"/>
        </member-entity>

        <alias-all entity-alias="beneficiaryContactMech"/>
        <alias-all entity-alias="telecomNumber">
            <exclude field="contactMechId"/>
        </alias-all>
        <alias name="contactMechTypeDescription" entity-alias="contactMechType" field="description"/>
    </view-entity>
    <view-entity entity-name="BeneficiaryWorkflowInstance" package="banqio.beneficiary">
        <member-entity entity-alias="instance" entity-name="commons.workflow.WorkflowInstance"/>
        <member-entity entity-alias="instanceStatus" entity-name="moqui.basic.StatusItem" join-from-alias="instance">
            <key-map field-name="statusId"/>
        </member-entity>
        <member-entity entity-alias="actionType" entity-name="moqui.basic.Enumeration" join-from-alias="instance">
            <key-map field-name="actionTypeEnumId"/>
        </member-entity>
        <member-entity entity-alias="workflow" entity-name="commons.workflow.Workflow" join-from-alias="instance">
            <key-map field-name="workflowId"/>
        </member-entity>
        <member-entity entity-alias="workflowType" entity-name="commons.workflow.WorkflowType" join-from-alias="workflow">
            <key-map field-name="workflowTypeId"/>
        </member-entity>
        <member-entity entity-alias="beneficiary" entity-name="banqio.beneficiary.Beneficiary" join-from-alias="instance">
            <key-map field-name="primaryKeyValue" related="beneficiaryId"/>
        </member-entity>
        <member-entity entity-alias="beneficiaryAddress" entity-name="banqio.beneficiary.BeneficiaryAddress" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="beneficiaryId"/>
        </member-entity>
        <member-entity entity-alias="country" entity-name="moqui.basic.Geo" join-from-alias="beneficiaryAddress" join-optional="true">
            <key-map field-name="countryGeoId"/>
        </member-entity>
        <member-entity entity-alias="beneficiaryType" entity-name="banqio.beneficiary.BeneficiaryType" join-from-alias="beneficiary">
            <key-map field-name="typeId"/>
        </member-entity>
        <member-entity entity-alias="typeBehaviour" entity-name="moqui.basic.Enumeration" join-from-alias="beneficiaryType">
            <key-map field-name="typeBehaviourEnumId"/>
        </member-entity>
        <member-entity entity-alias="beneficiaryStatus" entity-name="moqui.basic.StatusItem" join-from-alias="beneficiary">
            <key-map field-name="statusId"/>
        </member-entity>
        <member-entity entity-alias="preferredCurrency" entity-name="moqui.basic.Uom" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="preferredCurrencyId"/>
        </member-entity>
        <member-entity entity-alias="branch" entity-name="banqio.bank.Branch" join-from-alias="beneficiary" join-optional="true">
            <key-map field-name="swiftCode"/>
        </member-entity>
        <member-entity entity-alias="bank" entity-name="banqio.bank.Bank" join-from-alias="branch" join-optional="true">
            <key-map field-name="bankId"/>
        </member-entity>
        <member-entity entity-alias="inputUser" entity-name="moqui.security.UserAccount" join-from-alias="instance" join-optional="true">
            <key-map field-name="inputUserId"/>
        </member-entity>

        <alias-all entity-alias="instance"/>
        <alias-all entity-alias="beneficiary">
            <exclude field="inputUserId"/>
            <exclude field="creationDate"/>
        </alias-all>
        <alias-all entity-alias="beneficiaryAddress">
            <exclude field="beneficiaryId"/>
            <exclude field="creationDate"/>
        </alias-all>
        <alias name="statusDescription" entity-alias="instanceStatus" field="description"/>
        <alias name="workflowTypeId" entity-alias="workflowType" field="typeId"/>
        <alias name="actionTypeDescription" entity-alias="actionType" field="description"/>
        <alias name="bankId" entity-alias="bank" field="bankId"/>
        <alias name="bankName" entity-alias="bank" field="bankName"/>
        <alias name="countryGeoName" entity-alias="country" field="geoName"/>
        <alias name="countryGeoCodeAlpha2" entity-alias="country" field="geoCodeAlpha2"/>
        <alias name="countryGeoCodeAlpha3" entity-alias="country" field="geoCodeAlpha3"/>
        <alias name="countryGeoCodeNumeric" entity-alias="country" field="geoCodeNumeric"/>
        <alias name="typeName" entity-alias="beneficiaryType" field="typeName"/>
        <alias name="typeBehaviourEnumId" entity-alias="typeBehaviour" field="enumId"/>
        <alias name="typeBehaviourDescription" entity-alias="typeBehaviour" field="description"/>
        <alias name="beneficiaryStatusId" entity-alias="beneficiaryStatus" field="statusId"/>
        <alias name="beneficiaryStatusDescription" entity-alias="beneficiaryStatus" field="description"/>
        <alias name="preferredCurrencyDescription" entity-alias="preferredCurrency" field="description"/>
        <alias name="preferredCurrencyAbbreviation" entity-alias="preferredCurrency" field="abbreviation"/>
        <alias name="inputUserFullName" entity-alias="inputUser" field="userFullName"/>

        <entity-condition>
            <econdition field-name="workflowTypeId" value="WF_BEN"/>
        </entity-condition>
    </view-entity>

</entities>