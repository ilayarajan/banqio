                    <auto-parameters entity-name="moqui.basic.Enumeration"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="create" noun="Enumeration" type="java" location="com.netvariant.commons.basic.EnumerationService" method="createEnumeration">
        <in-parameters>
            <parameter name="enumTypeId"/>
            <parameter name="description"/>
        </in-parameters>
        <out-parameters>
            <parameter name="enumId"/>
        </out-parameters>
    </service>
    <service verb="update" noun="Enumeration" type="java" location="com.netvariant.commons.basic.EnumerationService" method="updateEnumeration">
        <in-parameters>
            <parameter name="enumId"/>
            <parameter name="description"/>
        </in-parameters>
        <out-parameters>
            <parameter name="enumId"/>
        </out-parameters>
    </service>
    <service verb="delete" noun="Enumeration" type="java" location="com.netvariant.commons.basic.EnumerationService" method="deleteEnumeration">
        <in-parameters>
            <parameter name="enumId"/>
        </in-parameters>
        <out-parameters/>
    </service>

