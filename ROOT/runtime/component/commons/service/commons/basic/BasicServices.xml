<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd">

    <!-- Language -->
    <service verb="find" noun="Language">
        <in-parameters>
            <auto-parameters entity-name="commons.basic.Language">
                <exclude field-name="disabled"/>
                <exclude field-name="lastUpdatedStamp"/>
            </auto-parameters>
        </in-parameters>
        <out-parameters>
            <parameter name="languageList" type="List">
                <parameter name="language" type="Map">
                    <auto-parameters entity-name="commons.basic.Language"/>
                </parameter>
            </parameter>
        </out-parameters>
        <actions>
            <entity-find entity-name="commons.basic.Language" list="languageList">
                <search-form-inputs paginate="false"/>
                <econdition field-name="disabled" value="N"/>
                <order-by field-name="languageName"/>
            </entity-find>
            <script>result.put("languageList", languageList)</script>
        </actions>
    </service>

    <!-- Enumeration -->
    <service verb="find" noun="Enumeration" type="java" location="com.netvariant.commons.basic.EnumerationService" method="findEnumerations">
        <in-parameters>
