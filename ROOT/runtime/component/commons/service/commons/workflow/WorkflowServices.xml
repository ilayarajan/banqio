<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd">

    <!-- Workflow type -->
    <service verb="find" noun="WorkflowType" type="java" location="com.netvariant.commons.workflow.WorkflowTypeService" method="findWorkflowTypes">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowType"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="pageNoLimit" type="Boolean" default="false"/>
            <parameter name="orderByField"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowTypeList" type="List">
                <parameter name="workflowType" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowType"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="find" noun="WorkflowTypeService" type="java" location="com.netvariant.commons.workflow.WorkflowTypeService" method="findWorkflowTypeServices">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowTypeService"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="pageNoLimit" type="Boolean" default="false"/>
            <parameter name="orderByField"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowTypeServiceList" type="List">
                <parameter name="workflowTypeService" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowTypeService"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="find" noun="WorkflowTypeAlertTemplate" type="java" location="com.netvariant.commons.workflow.WorkflowTypeService" method="findWorkflowTypeAlertTemplates">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowTypeAlertTemplateDetail"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="pageNoLimit" type="Boolean" default="false"/>
            <parameter name="orderByField"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowTypeAlertTemplateList" type="List">
                <parameter name="workflowTypeAlertTemplate" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowTypeAlertTemplateDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>

    <!-- Workflow -->
    <service verb="find" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflows">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowDetail"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="orderByField"/>
            <parameter name="filter"/>
            <parameter name="ignoreWorkflowId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="totalRows" type="Long"/>
            <parameter name="workflowList" type="List">
                <parameter name="workflow" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="create" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="createWorkflow">
        <in-parameters>
            <parameter name="workflowTypeId"/>
            <parameter name="statusFlowId"/>
            <parameter name="launchTypeEnumId"/>
            <parameter name="workflowName"/>
            <parameter name="description"/>
            <parameter name="reminderInterval" type="Integer"/>
            <parameter name="reminderIntervalUomId"/>
            <parameter name="reminderLimit" type="Integer"/>
            <parameter name="disabled"/>
            <parameter name="userGroupId" type="Object"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="update" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="updateWorkflow">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="workflowName"/>
            <parameter name="description"/>
            <parameter name="reminderInterval" type="Integer"/>
            <parameter name="reminderIntervalUomId"/>
            <parameter name="reminderLimit" type="Integer"/>
            <parameter name="disabled"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="duplicate" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="duplicateWorkflow">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="workflowName"/>
            <parameter name="description"/>
            <parameter name="reminderInterval" type="Integer"/>
            <parameter name="reminderIntervalUomId"/>
            <parameter name="reminderLimit" type="Integer"/>
            <parameter name="disabled"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="approve" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="approveWorkflow">
        <in-parameters>
            <parameter name="workflowId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="design" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="designWorkflow">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="modelData"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="draft" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="draftWorkflow">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="modelData"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="disable" noun="Workflow" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="disableWorkflow">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="disabled"/>
        </in-parameters>
        <out-parameters>
            <parameter name="workflowId"/>
        </out-parameters>
    </service>
    <service verb="find" noun="WorkflowVariable" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflowVariables">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="orderByField"/>
            <parameter name="filter"/>
        </in-parameters>
        <out-parameters>
            <parameter name="totalRows" type="Long"/>
            <parameter name="workflowVariableList" type="List">
                <parameter name="workflowVariable" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowVariableDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="create" noun="WorkflowVariable" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="createWorkflowVariable">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="variableTypeEnumId"/>
            <parameter name="variableName"/>
            <parameter name="description"/>
            <parameter name="defaultValue"/>
        </in-parameters>
        <out-parameters>
            <parameter name="variableId"/>
        </out-parameters>
    </service>
    <service verb="find" noun="WorkflowInitiator" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflowInitiators">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="orderByField"/>
            <parameter name="filter"/>
        </in-parameters>
        <out-parameters>
            <parameter name="totalRows" type="Long"/>
            <parameter name="workflowInitiatorList" type="List">
                <parameter name="workflowInitiator" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowInitiatorDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="create" noun="WorkflowInitiator" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="createWorkflowInitiator">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="userGroupId"/>
            <parameter name="fromDate"/>
            <parameter name="toDate"/>
        </in-parameters>
        <out-parameters>
            <parameter name="initiatorId"/>
        </out-parameters>
    </service>
    <service verb="expire" noun="WorkflowInitiator" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="expireWorkflowInitiator">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="initiatorId"/>
        </in-parameters>
    </service>
    <service verb="find" noun="WorkflowStats" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflowStats">
        <out-parameters>
            <parameter name="activeWorkflows" type="Long"/>
            <parameter name="pendingWorkflows" type="Long"/>
            <parameter name="disabledWorkflows" type="Long"/>
            <parameter name="totalWorkflows" type="Long"/>
            <parameter name="workflowTypes" type="List">
                <parameter name="workflowType" type="Map">
                    <parameter name="typeId"/>
                    <parameter name="typeName"/>
                    <parameter name="instanceCount"/>
                </parameter>
            </parameter>
            <parameter name="workflowStatuses" type="List">
                <parameter name="workflowStatus" type="Map">
                    <parameter name="statusId"/>
                    <parameter name="statusDescription"/>
                    <parameter name="instanceCount"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>

    <!-- Workflow instance -->
    <service verb="find" noun="WorkflowInstance" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflowInstances">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowInstanceDetail"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="orderByField"/>
            <parameter name="filter"/>
        </in-parameters>
        <out-parameters>
            <parameter name="totalRows" type="Long"/>
            <parameter name="workflowInstanceList" type="List">
                <parameter name="workflowInstance" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowInstanceDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="create" noun="WorkflowInstance" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="createWorkflowInstance">
        <in-parameters>
            <parameter name="workflowId"/>
            <parameter name="primaryKeyValue"/>
            <parameter name="actionTypeEnumId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="instanceId"/>
        </out-parameters>
    </service>
    <service verb="start" noun="WorkflowInstance" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="startWorkflowInstance" authenticate="false">
        <in-parameters>
            <parameter name="instanceId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="instanceId"/>
        </out-parameters>
    </service>
    <service verb="start" noun="ElapsedWorkflowInstances" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="startElapsedWorkflowInstances" authenticate="false"/>
    <service verb="abort" noun="WorkflowInstance" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="abortWorkflowInstance">
        <in-parameters>
            <parameter name="instanceId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="instanceId"/>
        </out-parameters>
    </service>
    <service verb="suspend" noun="WorkflowInstance" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="suspendWorkflowInstance">
        <in-parameters>
            <parameter name="instanceId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="instanceId"/>
        </out-parameters>
    </service>
    <service verb="resume" noun="WorkflowInstance" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="resumeWorkflowInstance">
        <in-parameters>
            <parameter name="instanceId"/>
        </in-parameters>
        <out-parameters>
            <parameter name="instanceId"/>
        </out-parameters>
    </service>
    <service verb="validate" noun="WorkflowInstanceAccess" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="validateWorkflowInstanceAccess">
        <in-parameters>
            <parameter name="instanceId"/>
        </in-parameters>
    </service>
    <service verb="update" noun="WorkflowInstanceVariable" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="updateWorkflowInstanceVariable">
        <in-parameters>
            <parameter name="instanceId"/>
            <parameter name="variableId"/>
            <parameter name="valueExpression"/>
        </in-parameters>
        <out-parameters>
            <parameter name="variableId"/>
            <parameter name="definedValue"/>
        </out-parameters>
    </service>
    <service verb="find" noun="WorkflowInstanceTask" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflowInstanceTasks">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowInstanceTaskDetail"/>
            <parameter name="pageIndex" type="Integer"/>
            <parameter name="pageSize" type="Integer"/>
            <parameter name="orderByField"/>
            <parameter name="filter"/>
        </in-parameters>
        <out-parameters>
            <parameter name="totalRows" type="Long"/>
            <parameter name="taskList" type="List">
                <parameter name="task" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowInstanceTaskDetail"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>
    <service verb="count" noun="WorkflowInstanceTask" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="countWorkflowInstanceTasks">
        <in-parameters>
            <auto-parameters entity-name="commons.workflow.WorkflowInstanceTaskDetail"/>
            <parameter name="filter"/>
        </in-parameters>
        <out-parameters>
            <parameter name="totalRows" type="Long"/>
        </out-parameters>
    </service>
    <service verb="update" noun="WorkflowInstanceTask" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="updateWorkflowInstanceTask">
        <in-parameters>
            <parameter name="sourceAppId"/>
            <parameter name="taskId"/>
            <parameter name="statusId"/>
            <parameter name="definedValue"/>
            <parameter name="comments"/>
            <parameter name="password"/>
        </in-parameters>
        <out-parameters>
            <parameter name="taskId"/>
        </out-parameters>
    </service>
    <service verb="find" noun="WorkflowInstanceTaskStats" type="java" location="com.netvariant.commons.workflow.WorkflowService" method="findWorkflowInstanceTaskStats">
        <out-parameters>
            <parameter name="workflowInstanceTaskStatuses" type="List">
                <parameter name="workflowInstanceTaskStatus" type="Map">
                    <auto-parameters entity-name="commons.workflow.WorkflowInstanceTaskStats"/>
                </parameter>
            </parameter>
        </out-parameters>
    </service>

</services>