    <moqui.basic.Enumeration enumTypeId="NumberComparisonOperator" enumId="NUM_EQUALS" description="Equals" sequenceNum="1"/>
    <moqui.basic.Enumeration enumTypeId="NumberComparisonOperator" enumId="NUM_NOT_EQUALS" description="Not Equals" sequenceNum="2"/>
