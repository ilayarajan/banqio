<?xml version="1.0" encoding="UTF-8"?>
<entity-facade-xml type="seed">

    <!-- Languages -->
    <CommonsLang languageId="EN" languageName="English" iconName="us.png" locale="en" rtl="N" disabled="N"/>
    <CommonsLang languageId="AR" languageName="العربية" iconName="lb.png" locale="ar" rtl="Y" disabled="N"/>

    <!-- Alert types -->
    <moqui.basic.EnumerationType enumTypeId="AlertType" description="Alert Type"/>
    <moqui.basic.Enumeration enumTypeId="AlertType" enumId="ALERT_EMAIL" description="Email" sequenceNum="1"/>
    <moqui.basic.Enumeration enumTypeId="AlertType" enumId="ALERT_SMS" description="SMS" sequenceNum="2"/>
    <moqui.basic.Enumeration enumTypeId="AlertType" enumId="ALERT_PUSH" description="Push Notification" sequenceNum="3"/>

    <!-- Alert templates -->
    <CommonsAlertTpl alertTypeEnumId="ALERT_SMS" templateId="AtResetPwd" templateName="Reset Password" body="Your new password is: {password}"/>
    <CommonsAlertTpl alertTypeEnumId="ALERT_SMS" templateId="AtOtp" templateName="One Time Password" body="Your one time password is: {password}"/>

    <!-- Text comparison operator -->
    <moqui.basic.EnumerationType enumTypeId="TextComparisonOperator" description="Text Comparison Operator"/>
    <moqui.basic.Enumeration enumTypeId="TextComparisonOperator" enumId="TXT_EQUALS" description="Equals" sequenceNum="1"/>
    <moqui.basic.Enumeration enumTypeId="TextComparisonOperator" enumId="TXT_NOT_EQUALS" description="Not Equals" sequenceNum="2"/>
