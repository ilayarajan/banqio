    <!-- API -->
    <service verb="send" noun="Sms" type="interface">
        <description>
            This is an interface service. It provides a protocol that implementations must follow.
        </description>
        <in-parameters>
            <parameter name="mobileNumber"/>
            <parameter name="message"/>
        </in-parameters>
        <out-parameters>
            <parameter name="transactionId"/>
            <parameter name="success" type="Boolean"/>
        </out-parameters>
    </service>
    <service verb="send" noun="Email" type="interface">
