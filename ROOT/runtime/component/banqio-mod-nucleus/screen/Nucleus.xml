<?xml version="1.0" encoding="UTF-8"?>
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/xml-screen-2.1.xsd"
        require-authentication="anonymous-view"
        include-child-content="true"
        default-menu-title="Banqio"
        menu-image="glyphicon glyphicon-globe"
        menu-image-type="icon">

    <subscreens default-item="Dashboard"/>

    <widgets>
        <subscreens-menu id="moduleMenu" type="popup"/>
        <subscreens-active/>
    </widgets>

</screen>
